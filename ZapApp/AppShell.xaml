<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ZapApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ZapApp.AppPages"
    Shell.FlyoutBehavior="Disabled"
    Title="ZapApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:StartPage}"
        Route="StartPage" />
    
    <ShellContent
    Title="Configuração"
    ContentTemplate="{DataTemplate local:ConfigPage}"
    Route="ConfigPage" />

    <ShellContent
 Title="SearchPage"
 ContentTemplate="{DataTemplate local:SearchPage}"
 Route="SearchPage" />

</Shell>
